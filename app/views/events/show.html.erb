<div class='clear'>
  <p id="notice"><%= notice %></p>

  <p>
    <b>Name:</b>
    <%= @event.name %>
  </p>

  <p>
    <b>Address:</b>
    <%= @event.address %>
  </p>

  <p>
    <b>Start date:</b>
    <%= @event.start_date %>
