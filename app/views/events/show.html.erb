<div class='admin-content'>
  <p id="notice"><%= notice %></p>

  <p>
    <b>Name:</b>
    <%= @event.name %>
  </p>

  <p>
    <b>Address:</b>
    <%= @event.address %>
  </p>

  <p>
    <b>City:</b>
    <%= @event.city %>
  </p>

  <p>
    <b>State:</b>
    <%= @event.state %>
  </p>

  <p>
    <b>Latitude:</b>
    <%= @event.latitude %>
  </p>

  <p>
    <b>Longitude:</b>
    <%= @event.longitude %>
  </p>

  <p>
    <%= link_to "Map Current Location", google_map_url(@event), :target => :new %>
  </p>

  <p>
    <b>Start date:</b>
    <%= @event.start_date %>
  </p>

  <p>
    <b>End date:</b>
    <%= @event.end_date %>
  </p>

  <p>
    <b>Description:</b>
    <%= @event.description %>
  </p>


  <%= link_to 'Edit', edit_event_path(@event) %> |
  <%= link_to 'Back', events_path %>
</div>